#include <stdio.h>

// Функція для обчислення кількості послідовностей
int countSequences(int n) {
    // Ініціалізуємо динамічний масив для збереження кількості послідовностей
    int dp[10001] = {0};

    // Початкові значення
    dp[1] = 2;
    dp[2] = 4;

    // Обчислюємо кількість послідовностей
    for (int i = 3; i <= n; i++) {
        dp[i] = (dp[i-1] + dp[i-2]) % 12345;
    }

    return dp[n];
}

int main() {
    // Оголошуємо змінну
    int n;
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    // Знаходимо кількість послідовностей
    int result = countSequences(n);

    // Виводимо результат
    printf("Кількість шуканих послідовностей: %d\n", result);

    return 0;
}
